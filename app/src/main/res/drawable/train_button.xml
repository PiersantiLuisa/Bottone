<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#07d5ff" />
            <stroke
                android:width="10dp"
                android:color="#1aade2" />
            <corners
                android:radius="300dp"/>

            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />


        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#076aff"
                android:endColor="#003399"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#003399" />
            <corners
                android:radius="300dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>